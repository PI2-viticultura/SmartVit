{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {},
    "overrides": [
        {
            "files": [
                "setupTests.js",
                "*.spec.js",
                "serviceWorker.js"
            ],
            "rules": {
                "no-unused-expressions": "off"
            }
        }
    ]
}